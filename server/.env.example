# Порт на котором запускается сервер
PORT=3001

# Разрешенные origins для CORS (через запятую)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Настройки веб-сокетов
PING_TIMEOUT=10000
PING_INTERVAL=5000

# URL клиента
CLIENT_URL=http://localhost:5173

# URI базы данных MongoDB
MONGODB_URI=mongodb://localhost:27017/dcpoker

# Секретный ключ JWT
JWT_SECRET=your_strong_secret_key_here

# Время жизни токена JWT
JWT_EXPIRES_IN=7d 